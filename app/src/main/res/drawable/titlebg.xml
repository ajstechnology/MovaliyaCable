<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <solid android:color="#503737" />
        </shape>
    </item>

    <item>
        <rotate
            android:fromDegrees="50"
            android:pivotX="150%"
            android:pivotY="120%"
            android:toDegrees="50">
            <shape android:shape="rectangle">
                <solid android:color="#503737" />
            </shape>
        </rotate>
    </item>

</layer-list>